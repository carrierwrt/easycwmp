# easycwmp uci configuration

config local
	option interface wan
	option port 7547
	option ubus_socket /var/run/ubus.sock
	option date_format %FT%T%z
	option username ''
	option password ''

config acs
	option scheme http
	option username ''
	option password ''
	option hostname ''
	option port 7547
	option path / 
	option parameter_key ''
	option periodic_enable '1'
	option periodic_interval '100'

